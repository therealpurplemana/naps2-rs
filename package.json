{
  "name": "naps2-rust-bindings",
  "version": "0.1.0",
  "description": "Rust bindings for NAPS2.Sdk on macOS",
  "scripts": {
    "build:cs": "cd csharp-helper && dotnet build",
    "build:rust": "cargo build",
    "build": "npm run build:cs && npm run build:rust",
    "start": "cargo run",
    "generate-bindings": "node scripts/generate-bindings.js",
    "test": "cargo test"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
} 